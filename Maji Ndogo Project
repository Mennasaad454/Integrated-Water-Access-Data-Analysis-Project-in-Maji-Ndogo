    "%%sql\n",
    "# count how many employees live in each town using the employee table.\n",
    "SELECT town_name,\n",
    "    COUNT(employee_name) AS number_of_people\n",
    "FROM md_water_services.employee\n",
    "GROUP BY town_name\n",
    "LIMIT 5;\n"
   ]
